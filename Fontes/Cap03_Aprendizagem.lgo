APRENDA CONTAGEM1
  ATRIBUA "I 1 
  ENQUANTO [:I <= 5] [
    ESCREVA :I
    ATRIBUA "I :I + 1 
  ]
FIM

APRENDA CONTAGEM2
  FAÇAPARA [I 1 5] [
    ESCREVA :I
  ]
FIM

APRENDA ESPIRAL :TAMANHO :ANGULO :NUMERO
  SE :NUMERO = 0 [
    PARE
  ]
  PF :TAMANHO
  PD :ANGULO
  ESPIRAL (:TAMANHO + 8) :ANGULO (:NUMERO - 1)
FIM

APRENDA ESTRELA40
  FAÇAPARA [I 1 40] [
    PT 200 
    PE 170
  ]
FIM

APRENDA FLOR
  TAT
  REPITA 8 [PETALA PD 45]
FIM

APRENDA FLORAL :N
  DEVOLVA (
    SE (OU :N < 1 :N > 7) [
      ESCREVA [Use valores entre 1 e 7]
    ][
      REPITA 8 [
        PD 45 
        REPITA :N [
          REPITA 90 [
            PF 2 
            PD 2
          ] 
          PD 90
        ]
      ]
    ]
  )
FIM

APRENDA IMPAR :N
  DEVOLVA (SE (RESTO :N 2) <> 0 [ESCREVA [Ok]])
FIM

APRENDA MAXIMO :A :B
  DEVOLVA (SE :A > :B [ESC :A] [ESC :B])
FIM

APRENDA MEIOCIRC
  REPITA 90 [PF 1 PD 1]
FIM

APRENDA PAR :N
  DEVOLVA (SE (RESTO :N 2) = 0 [ESCREVA [Ok]] [ESCREVA [Erro]])
FIM

aprenda PENTA1
