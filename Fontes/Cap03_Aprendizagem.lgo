  ATRIBUA "I 1 
  ENQUANTO [:I <= 5] [
    PF 100
    PD 144 
  ]
FIM

APRENDA PETALA
  MEIOCIRC
  PD 90
  MEIOCIRC
FIM

APRENDA POLIS :TAMANHO :LADO
  REPITA :LADO [PF :TAMANHO PD 360 / :LADO]
FIM

aprenda QUAD2 :TAMANHO
  REPITA 4 [PF :TAMANHO PD 90]
fim

APRENDA RODAFLOR
  FAÇAPARA [I 1 6] [ 
    ATRIBUA "I 1 
    ENQUANTO [:I <= 120] [
      PF 1 PD 1
      ATRIBUA "I :I + 1
    ]
    PD 60 
    REPITA 180 [
      PF 1 PD 1
    ] 
    PD 60
  ]
FIM

APRENDA TESTE_E :N
  DEVOLVA (
    SE (E :N >= 1 :N <= 9) [
      ESC [Valor está na faixa de 1 a 9.]
    ][
      ESC [Valor não está na faixa de 1 a 9.]
    ]
  )
FIM

APRENDA TESTE_NAO :V
  DEVOLVA (
    SE (NÃO :V > 5) [
      ESC POTÊNCIA :V 2
    ]
  )
FIM

APRENDA TESTE_OU :TEMPO
  DEVOLVA (
    SE (OU MAIÚSCULAS :TEMPO = "FRIO" MAIÚSCULAS :TEMPO = "CHUVOSO") [
      ESC [Tempo ruim, proteja-se.]
    ][
      ESC [Tempo bom, aproveite.]
    ]
  )
FIM

APRENDA TRELICA :X
  SE :X = 0 [PARE]
  REPITA 4 [TRIANGULAR PF 40 PD 90]
  PD 90 PF 40 PE 90
  TRELICA :X - 1
FIM

APRENDA TRIANGULAR
  PF 40 PD 135 
  PF 40 / RAIZQ 2 PD 90 
  PF 40 / RAIZQ 2 PD 135
FIM

